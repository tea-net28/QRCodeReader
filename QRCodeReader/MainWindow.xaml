<Window x:Class="QRCodeReader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QRCodeReader"
        mc:Ignorable="d"
        Title="MainWindow" Height="540" Width="960">
    <Grid>
        <Image x:Name="_boxCamera" HorizontalAlignment="Left" Height="360" Margin="10,10,0,0" VerticalAlignment="Top" Width="640"/>
        <Label Content="QR Text" HorizontalAlignment="Left" Margin="10,400,0,0" VerticalAlignment="Top" FontSize="16"/>
        <TextBox x:Name="_boxText" HorizontalAlignment="Left" Margin="10,435,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="500" FontSize="14" Height="50"/>
        <Label x:Name="_boxText2" Content="Result" HorizontalAlignment="Left" Margin="545,442,0,0" VerticalAlignment="Top" FontSize="16" Width="200" Height="36"/>

    </Grid>
</Window>
